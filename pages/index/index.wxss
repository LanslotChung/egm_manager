/**index.wxss**/
page{
  background-color: rgba(245, 245, 245, 0.8);
  color: #232323;
  height: 100vh;
}
.loading{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}
.loading image{
  display: block;
  height: 100vh;
}
.loading .iconjiazaizhong{
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -100rpx;
  margin-top: -200rpx;
  text-align: center;
  color: #e0e0e0;
  line-height: 200rpx;
  font-size: 100rpx;
  animation: myfirst 2s infinite linear;
}
.box-0{
  width: 750rpx;
  height: 420rpx;
  position: relative;
}
.box-0-0{
  width: 710rpx;
  height: 380rpx;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}
.box-0-0 image{
  display: block;
  width: 100%;
  height: 100%;
}
.box-0-1{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  height: 300rpx;
  padding: 60rpx 0 0;
  position: relative;
  z-index: 3;
}
.box-0-1-0{
  width: 150rpx;
  height: 150rpx;
  margin-bottom: 20rpx;
}
.box-0-1-0 image{
  width: 100%;
  height: 100%;
  display: block;
  border-radius: 50%;
}
.box-0-1-1{
  font-size: 38rpx;
  color: #fff;
  line-height: 40rpx;
  margin-bottom: 20rpx;
}
.box-0-1-2{
  font-size: 28rpx;
  color: #fff;
  line-height: 36rpx;
}

.box-1{
  font-size: 32rpx;
  padding: 40rpx 25rpx 10rpx;
}
.box-1-0{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #161616;
}
.box-1-1{
  width: 44rpx;
  margin-left: 8rpx;
  border-radius: 4rpx;
  height: 8rpx;
  background-color: #3c7ef0;
}
.box-2{
  margin: 10rpx 25rpx;
  border-radius: 14rpx;
  overflow: hidden;
}
.box-2-0-0{
  margin-bottom: 20rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.box-2-0-0-0{
  height:280rpx;
  background-color: #f0f0f0;
  position: relative;
}
.box-2-0-0-0-0{
  height: 280rpx;
}
.box-2-0-0-0-0 image{
  display: block;
  width: 100%;
  height: 100%;
}
.box-2-0-0-0-1{
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -66rpx;
  margin-top: -66rpx;
  height: 100rpx;
  width: 100rpx;
  padding: 16rpx;
  border-radius: 70rpx;
  background-color: rgba(74,37,150,.2);
}
.box-2-0-0-0-1-0{
  width: 96rpx;
  height: 96rpx;
  border: 2rpx solid #fff;
  color: #fff;
  border-radius: 50rpx;
  font-size: 20rpx;
  line-height: 96rpx;
  text-align: center;
}
.box-2-0-0-1{
  padding: 10rpx 20rpx;
  display: flex;
  background-color: rgba(255, 255, 255, 1);
}
.box-2-0-0-1-0{
  width: 80rpx;
  height: 80rpx;
  border-radius: 8rpx;
}
.box-2-0-0-1-0 image{
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.box-2-0-0-1-1{
  flex: 1;
  padding-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.box-2-0-0-1-1-0{
  font-size: 28rpx;
  color: #161616;
}
.box-2-0-0-1-1-1{
  font-size: 24rpx;
  color: #161616;
}
.box-2-0-0-1-1-1{
  display: flex;
  padding-bottom: 6rpx;
}
.box-2-0-0-1-1-1-0{
  background-color: #ebebeb;
  color: #161616;
  font-size: 16rpx;
  height: 24rpx;
