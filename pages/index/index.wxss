  margin-right: 10rpx;
}
.box-2-0-0-1-2{
  text-align: right;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.box-2-0-0-1-2-0,.box-2-0-0-1-2-1{
  display: flex;
  font-size: 22rpx;
  justify-content: flex-end;
  height: 40rpx;
  line-height: 40rpx;
}
.box-2-0-0-1-2-0 .iconloc-s{
  font-size: 28rpx;
  margin-right: 6rpx;
  height: 40rpx;
  line-height: 40rpx;
  color: #4274f2;
}
.box-2-0-0-1-2-1{
  color: #161616;
  font-size: 18rpx;
}
.box-2-0-0-1-2-1 .iconliulan{
  margin-right: 6rpx;
  font-size: 24rpx;
  height: 40rpx;
  line-height: 44rpx;
}
.box-1-2{
  width: 44rpx;
  margin-left: 42rpx;
  border-radius: 4rpx;
  height: 8rpx;
  background-color: #3c7ef0;
}